{
    "manifest_version": 2,
    "name": "DDT - Don't Dupe Tabs",
    "short_name": "DDT",
    "description": "Tries to help people with a compulsive tabs opening habit by preventing duplicated tabs",
    "version": "0.2",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon-open.png"
    },
    "background": {
        "scripts": [
            "eventPage.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}