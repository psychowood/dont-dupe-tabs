{
    "manifest_version": 2,
    "name": "DDT - DontDupeTabs",
    "description": "Tries to help people with a compulsive tabs opening habit by preventing duplicated tabs",
    "version": "0.1",
    "icons": {
        "192": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon-open.png"
    },
    "background": {
        "scripts": [
            "eventPage.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}